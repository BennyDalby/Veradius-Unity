a:link 			{font-weight: regular; color: #000000; text-decoration:none; font-family: Philips Healthcare Font; font-size: 14px;}	/* unvisited link */
a:visited 	{font-weight: regular; color: #000000; text-decoration:none; font-family: Philips Healthcare Font; font-size: 14px;}	/* visited link */
a:hover 		{font-weight: regular; color: #000000; text-decoration:underline; font-family: Philips Healthcare Font; font-size: 14px;}	/* mouse over link */
a:active 		{font-weight: regular; color: #000000; text-decoration:none; font-family: Philips Healthcare Font; font-size: 14px;}	/* selected link */

a.new ({font-weight: regular; color: #000000; text-decoration:none; font-family: Philips Healthcare Font; font-size: 20px;}

