#footer {
		font-family: Philips Healthcare Font;
		font-size:20px;
		width:1123px;
		height:3000px;
		background-color:#ddd;
		position:fixed;
		left:0px;
		top:548px;
		border-collapse:collapse;
		padding:0;
		margin:0;
		z-index:999;
}


/*------------------------------------------------*/

#popup_container.style_1 {
				font-family: Philips Healthcare Font;
				font-size: 14px;
				color: #000;
				background: #fff;
				border-color: #3778AE;
				width:600px;
			}

 popup style		
			#popup_container.style_1 #popup_title {
				color: #FFF;
				font-weight: normal;
				text-align: left;
				background: #76A5CC;
				border: solid 1px #005294;
				padding-left: 1em;
			}
		
			#popup_container.style_1 #popup_content {
				background: none;
			}
			
			#popup_container.style_1 #popup_message {
				padding-left: 0em;
			}
			
			#popup_container.style_1 INPUT[type='button'] {
				border: outset 2px #76A5CC;
				color: #A4C6E2;
				background: #3778AE;
			}



